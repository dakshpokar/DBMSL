db.createCollection('StockData')

db.StockData.insert({name: "Daksh", age: 56, email: "dakshpokar@gmail.com", stocks: {stock_1: {name: "Reliance", price: 450, quantity: 6}, stock_2: {name: "Mahindra", price: 234, quantity: 6}}})
WriteResult({ "nInserted" : 1 })

